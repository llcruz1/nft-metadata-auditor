{
  "name": "NFT Metadata Auditor",
  "description": "A plugin to audit NFT's metadata.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "NFA - MA"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [ 
    "tabs",
    "notifications"
 ],
  "icons": {
    "16": "logo512.png",
    "48": "logo512.png",
    "128": "logo512.png"
  }
}